{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s | %(name)s | %(levelname)s | %(message)s"
        }
    },
    "handlers": {
        "supplementary_file_handler_error": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "ERROR",
            "filename": "",
            "mode": "a"
        },
        "supplementary_file_handler_info": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "INFO",
            "filename": "",
            "mode": "a"
        },
        "parsing_file_handler_error": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "ERROR",
            "filename": "",
            "mode": "a"
        },
        "parsing_file_handler_info": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "INFO",
            "filename": "",
            "mode": "a"
        },
        "variant_search_file_handler_error": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "ERROR",
            "filename": "",
            "mode": "a"
        },
        "variant_search_file_handler_info": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "INFO",
            "filename": "",
            "mode": "a"
        },
        "w3c_file_handler_error": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "ERROR",
            "filename": "",
            "mode": "a"
        },
        "w3c_file_handler_info": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "INFO",
            "filename": "",
            "mode": "a"
        },
        "w3c_file_handler_body_error": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "ERROR",
            "filename": "",
            "mode": "a"
        },
        "submission_file_handler_info": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "INFO",
            "filename": "",
            "mode": "a"
        },
        "submission_file_handler_body_error": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "ERROR",
            "filename": "",
            "mode": "a"
        },
        "main_file_handler_info": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "INFO",
            "filename": "",
            "mode": "a"
        },
        "main_file_handler_body_error": {
            "class": "logging.FileHandler",
            "formatter": "standard",
            "level": "ERROR",
            "filename": "",
            "mode": "a"
        }
    },
    "loggers": {
        "supplementary_error_logger": {
            "handlers": [
                "supplementary_file_handler_error"
            ],
            "level": "ERROR",
            "propagate": true
        },
        "supplementary_info_logger": {
            "handlers": [
                "supplementary_file_handler_info"
            ],
            "level": "INFO",
            "propagate": false
        },
        "parsing_error_logger": {
            "handlers": [
                "parsing_file_handler_error"
            ],
            "level": "ERROR",
            "propagate": true
        },
        "parsing_info_logger": {
            "handlers": [
                "parsing_file_handler_info"
            ],
            "level": "INFO",
            "propagate": false
        },
        "variant_search_error_logger": {
            "handlers": [
                "variant_search_file_handler_error"
            ],
            "level": "ERROR",
            "propagate": true
        },
        "variant_search_info_logger": {
            "handlers": [
                "variant_search_file_handler_info"
            ],
            "level": "INFO",
            "propagate": false
        },
        "w3c_error_logger": {
            "handlers": [
                "w3c_file_handler_error"
            ],
            "level": "ERROR",
